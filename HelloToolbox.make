#   File:       HelloToolbox.make#   Target:     HelloToolbox#   Created:    Tuesday, June 6, 2017 02:26:40 PMMAKEFILE        = HelloToolbox.make쩗ondoBuild    = {MAKEFILE}  # Make blank to avoid rebuilds when makefile is modifiedObjDir          = :obj:Includes        = Sym-PPC         = -sym offSym-68K         = -sym offPPCCPlusOptions = {Includes} {Sym-PPC} CPlusOptions    = {Includes} {Sym-68K} -model near ### Source Files ###SrcFiles        =  				  hello.cpp### Object Files ###ObjFiles-PPC    =  				  "{ObjDir}hello.cpp.x"ObjFiles-68K    =  				  "{ObjDir}hello.cpp.o"### Libraries ###LibFiles-PPC    =  				  "{SharedLibraries}InterfaceLib" 				  "{SharedLibraries}StdCLib" 				  "{SharedLibraries}MathLib" 				  "{PPCLibraries}StdCRuntime.o" 				  "{PPCLibraries}PPCCRuntime.o" 				  "{PPCLibraries}PPCToolLibs.o" 				  "{PPCLibraries}MrCPlusLib.o" 				  "{PPCLibraries}MrCIOStreams.o"LibFiles-68K    =  				  "{Libraries}MathLib.o" 				  "{CLibraries}StdCLib.o" 				  "{CLibraries}IOStreams.o" 				  "{CLibraries}CPlusLib.o" 				  "{Libraries}MacRuntime.o" 				  "{Libraries}IntEnv.o" 				  "{Libraries}ToolLibs.o" 				  "{Libraries}Interface.o"### Default Rules ###.cpp.x    .cpp  {쩗ondoBuild}	{PPCCPlus} {depDir}{default}.cpp -o {targDir}{default}.cpp.x {PPCCPlusOptions}.cpp.o    .cpp  {쩗ondoBuild}	{CPlus} {depDir}{default}.cpp -o {targDir}{default}.cpp.o {CPlusOptions}### Build Rules ###HelloToolbox  컴  {ObjFiles-PPC} {LibFiles-PPC} {쩗ondoBuild}	PPCLink 		-o {Targ} 		{ObjFiles-PPC} 		{LibFiles-PPC} 		{Sym-PPC} 		-mf -d 		-t 'APPL' 		-c '????'HelloToolbox  컴  {ObjFiles-68K} {LibFiles-68K} {쩗ondoBuild}	ILink 		-o {Targ} 		{ObjFiles-68K} 		{LibFiles-68K} 		{Sym-68K} 		-mf -d 		-t 'APPL' 		-c '????' 		-model near 		-state rewrite 		-compact -pad 0	If "{Sym-68K}" =~ /-sym [nNuU]/		ILinkToSYM {Targ}.NJ -mf -sym 3.2 -c 'sade'	EndHelloToolbox  컴  hello.rsrc {쩗ondoBuild}	Echo "Include "hello.rsrc";" | Rez -o {Targ} -append### Required Dependencies ###"{ObjDir}hello.cpp.x" "{ObjDir}hello.cpp.o"    hello.cpp### Optional Dependencies ###### Build this target to generate "include file" dependencies. ###Dependencies    $OutOfDate	MakeDepend 		-append {MAKEFILE} 		-ignore "{CIncludes}" 		-objdir "{ObjDir}" 		-objext .x 		-objext .o 		{Includes} 		{SrcFiles}